    Return Value
    End Function

    Public Function BlankZeroAndPos(ByVal Value As Decimal)
    if Value &gt;= 0 then
    Return ""
    end if
    Return Value
    End Function

    Public Function BlankNeg(ByVal Value As Decimal)
    if Value &lt; 0 then
    Return ""
    end if
    Return Value
    End Function

