    "al.enableCodeAnalysis": true,    
    "al.codeAnalyzers": [
        "${AppSourceCop}",
        "${CodeCop}",
        "${PerTenantExtensionCop}",
        "${UICop}"
    ],
    "al.ruleSetPath": ".\\Company.ruleset.json"
